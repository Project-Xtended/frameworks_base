<resources>
    <!-- HBM type of UDFPS overlay.
            0 - GLOBAL HBM
            1 - LOCAL HBM
    -->
    <integer name="config_udfps_hbm_type">1</integer>

    <!-- Color of the UDFPS pressed view -->
    <color name="config_udfpsColor">#ffffffff</color>

    <!-- Max visible notification icons -->
    <integer name="config_maxVisibleNotificationIcons">5</integer>
    <integer name="config_maxVisibleNotificationIconsWhenLock">6</integer>

    <!-- On-The-Go -->
    <integer name="quick_settings_onthego_dialog_short_timeout">3000</integer>
    <integer name="quick_settings_onthego_dialog_long_timeout">6000</integer>

    <!-- Udfps vendor code -->
    <integer name="config_udfps_vendor_code">22</integer>

    <!-- FPSInfoService FPS node file path -->
    <string name="config_fpsInfoSysNode" translatable="false"></string>

    <!-- Whether usage of the proximity sensor during doze is supported -->
    <bool name="doze_proximity_sensor_supported">true</bool>

    <!-- Whether to pulse onFingerDown actions while in AOD. -->
    <bool name="config_pulseOnFingerDownInAod">false</bool>

    <!-- SystemUI Google -->
    <bool name="flag_monet">true</bool>
    <bool name="flag_smartspace">true</bool>

    <!-- Custom SystemUI Google -->
    <string name="config_statusBarComponent">org.pixelexperience.systemui.statusbar.phone.StatusBarGoogle</string>
    <string name="config_systemUIFactoryComponent">org.pixelexperience.systemui.SystemUIGoogleFactory</string>
    <string name="config_systemUIVendorServiceComponent">org.pixelexperience.systemui.GoogleServices</string>

</resources>
